#!/bin/bash

if [ "$1" = "release" ]; then
    open https://learnosity.atlassian.net/secure/RapidBoard.jspa?rapidView=29&quickFilter=260
elif [ -n "$1" ]; then
    open https://learnosity.atlassian.net/secure/QuickSearch.jspa?searchString=$1
else
    open https://learnosity.atlassian.net/secure/RapidBoard.jspa?rapidView=54&view=detail
fi

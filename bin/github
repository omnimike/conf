#!/bin/bash

if [ "$1" = "questions" ]; then
    open https://github.com/Learnosity/API
elif [ "$1" = "editor" ]; then
    open https://github.com/Learnosity/Editor-Responses
elif [ "$1" = "author" ]; then
    open https://github.com/Learnosity/authorapi
elif [ "$1" = "schemas" ]; then
    open https://github.com/Learnosity/api-schemas
elif [ "$1" = "spokenmath" ]; then
    open https://github.com/Learnosity/spokenmath
elif [ "$1" = "mathcore" ]; then
    open https://github.com/Learnosity/mathcore
elif [ -n "$1" ]; then
    open https://github.com/Learnosity?utf8=%E2%9C%93&query=$1
else
    open https://github.com/Learnosity
fi
